{
    "implicitDependencies": {
        "angular.json": "*",
        "package.json": "*",
        "tslint.json": "*",
        ".eslintrc.json": "*",
        "nx.json": "*",
        "tsconfig.json": "*"
    },
    "affected": {
        "defaultBase": "master"
    },
    "npmScope": "solucrea",
    "cli": {
        "defaultCollection": "@angular-eslint/schematics"
    },
    "defaultProject": "solucrea",
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    }
}
